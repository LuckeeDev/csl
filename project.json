{
	"targets": {
		"version": {
			"executor": "@jscutlery/semver:version",
			"options": {
				"syncVersions": true,
				"push": true,
				"noVerify": true,
				"rootChangelog": true,
				"commitMessageFormat": "release(${projectName}): 🏫 release version ${version}"
			}
		}
	},
	"root": ".",
	"tags": []
}
