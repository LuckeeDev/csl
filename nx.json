{
	"npmScope": "csl",
	"affected": {
		"defaultBase": "main"
	},
	"implicitDependencies": {
		"workspace.json": "*",
		"package.json": {
			"dependencies": "*",
			"devDependencies": "*"
		},
		"tsconfig.base.json": "*",
		".eslintrc.json": "*",
		"nx.json": "*"
	},
	"tasksRunnerOptions": {
		"default": {
			"runner": "@nrwl/workspace/tasks-runners/default",
			"options": {
				"cacheableOperations": ["build", "lint", "test", "e2e"]
			}
		}
	},
	"projects": {
		"api": {
			"tags": []
		},
		"client": {
			"tags": []
		},
		"client-e2e": {
			"tags": [],
			"implicitDependencies": ["client"]
		},
		"google": {
			"tags": []
		},
		"maintenance": {
			"tags": []
		},
		"shared": {
			"tags": []
		},
		"strapi": {
			"tags": []
		},
		"types": {
			"tags": []
		},
		"ui": {
			"tags": []
		},
		"workspace": {
			"tags": []
		}
	},
	"targetDependencies": {
		"build": [
			{
				"target": "build",
				"projects": "dependencies"
			}
		]
	}
}
