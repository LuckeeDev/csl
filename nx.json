{
	"npmScope": "csl",
	"affected": {
		"defaultBase": "main"
	},
	"implicitDependencies": {
		"workspace.json": "*",
		"package.json": {
			"dependencies": "*",
			"devDependencies": "*"
		},
		"tsconfig.base.json": "*",
		".eslintrc.json": "*",
		"nx.json": "*"
	},
	"tasksRunnerOptions": {
		"default": {
			"runner": "@nrwl/nx-cloud",
			"options": {
				"cacheableOperations": ["build", "lint", "test", "e2e"],
				"parallel": 1,
				"accessToken": "MDgzYTQ2YjEtYWViZS00OGFkLWEyNzgtNjA3MmJhODUzODMxfHJlYWQtd3JpdGU="
			}
		}
	},
	"targetDependencies": {
		"build": [
			{
				"target": "build",
				"projects": "dependencies"
			}
		]
	},
	"cli": {
		"defaultCollection": "@nrwl/next"
	},
	"generators": {
		"@nrwl/next": {
			"application": {
				"style": "scss",
				"linter": "eslint"
			}
		}
	}
}
