{
	"projectType": "library",
	"root": "libs/strapi",
	"sourceRoot": "libs/strapi/src",
	"prefix": "csl",
	"targets": {
		"serve": {
			"executor": "@nrwl/workspace:run-commands",
			"options": {
				"command": "yarn --cwd ./apps/strapi develop"
			}
		},
		"build": {
			"executor": "@nrwl/workspace:run-commands",
			"options": {
				"command": "yarn --cwd ./apps/strapi build"
			}
		},
		"start": {
			"executor": "@nrwl/workspace:run-commands",
			"options": {
				"command": "yarn --cwd ./apps/strapi start"
			}
		},
		"lint": {
			"executor": "@nrwl/linter:eslint",
			"options": {
				"lintFilePatterns": [
					"libs/strapi/src/**/*.ts",
					"libs/strapi/src/**/*.html"
				]
			},
			"outputs": ["{options.outputFile}"]
		},
		"test": {
			"executor": "@nrwl/jest:jest",
			"outputs": ["coverage/libs/strapi"],
			"options": {
				"jestConfig": "libs/strapi/jest.config.js",
				"passWithNoTests": true
			}
		}
	},
	"tags": []
}
