{
	"version": 2,
	"projects": {
		"api": "apps\\api",
		"client": "apps\\client",
		"client-e2e": "apps\\client-e2e",
		"google": "libs\\google",
		"shared": "libs\\shared",
		"strapi": "libs\\strapi",
		"types": "libs\\types",
		"ui": "libs\\ui",
		"web": "apps/web",
		"web-e2e": "apps/web-e2e",
		"workspace": "."
	},
	"cli": {
		"defaultCollection": "@nrwl/angular"
	},
	"generators": {
		"@nrwl/angular:application": {
			"style": "scss",
			"linter": "eslint",
			"unitTestRunner": "jest",
			"e2eTestRunner": "cypress"
		},
		"@nrwl/angular:library": {
			"style": "scss",
			"linter": "eslint",
			"unitTestRunner": "jest"
		},
		"@nrwl/angular:component": {
			"style": "scss"
		},
		"@nrwl/react": {
			"application": {
				"babel": true
			}
		},
		"@nrwl/next": {
			"application": {
				"style": "styled-components",
				"linter": "eslint"
			}
		}
	},
	"defaultProject": "client"
}
